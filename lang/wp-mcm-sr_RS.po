#: include/class-wp-mcm-shortcodes.php:155
#@ wp-mcm
msgid "<br/>This plugin supports the shortcodes as shown above as well as in all upper and lower case."
msgstr "<br/>Ovaj plugin podržava shortcode-ove kao što su ovi prikazani iznad, ali i sve druge, napisane velikim i malim slovima.  "

#: include/class-wp-mcm-shortcodes.php:156
#, php-format
#@ wp-mcm
msgid "<br/>More information can be found <a href=\"%s\">here</a>."
msgstr "<br/>Više informacija možete naći <a href=\"%s\">ovde</a>."

#: include/class-wp-mcm-shortcodes.php:157
#, php-format
#@ wp-mcm
msgid "<br/>If you have suggestions to improve this plugin, please leave a comment on this same <a href=\"%s\">page</a>."
msgstr "<br/>Ako imate predloge za unapređivanje ovog plugin-a, ostavite komentar na ovoj  <a href=\"%s\">stranici</a>."

